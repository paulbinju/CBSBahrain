<script src="@Url.Content("~/assets/js/jquery.min.js")"></script>
<link href="@Url.Content("~/assets/plugins/datatables/jquery.dataTables.min.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/assets/plugins/datatables/buttons.bootstrap.min.css")" rel="stylesheet" type="text/css" />
<link href="@Url.Content("~/assets/plugins/datatables/dataTables.bootstrap.min.css")" rel="stylesheet" type="text/css" />
<div class="content-page">
    <div class="content">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-primary">
                        <div class="panel-body">
                            <h4 class="page-title">Fuel Station</h4>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="page-content-wrapper ">
            <div class="container">
                <div class="row">
                    <div class="col-md-12">
                        <div class="panel panel-primary">
                            <div class="panel-body">


                                <table id="datatable" class="table table-striped table-bordered">
                                    <thead>
                                        <tr>
                                            <td>ID</td>
                                            <th title="Field #1">
                                                Vehicle Name
                                            </th>
                                           
                                            <th>Odometer</th>
                                            <th title="Field #4">
                                                Fuel Type
                                            </th>
                                            <th title="Field #5">
                                                Filling Date
                                            </th>
                                            <th>Quantity (Litre)</th>
                                            <th  style="text-align:right">Amount (@Session["Currency"])</th>

                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var fc in ViewBag.FuelConsumptiton)
                                        {
                                            <tr>
                                                <td>@fc.FuelConsumptionID</td>
                                                <td>@fc.VehicleNoName - @fc.Brand</td>
                                                <td>@fc.Odometer</td>
                                                <td>@fc.FuelType</td>
                                                <td>@fc.FillingDate</td>
                                                <td>@fc.Quantity</td>
                                                <td style="text-align:right">@fc.Amount</td>
                                                 </tr>
                                           
                                        }

                                    </tbody>
                                </table>





                            </div>
                        </div>
                    </div>

                </div> <!-- End Row -->




            </div><!-- container -->

        </div> <!-- Page content Wrapper -->

    </div> <!-- content -->


</div>


<script>
    $(document).ready(function () {
        var table = $('#datatable').DataTable();
        table
            .order([0, 'desc'])
            .draw();
        $('#datatable').attr('data-page-length', 50);
    });
</script>


